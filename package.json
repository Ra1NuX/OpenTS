    "acorn-walk": "^8.3.4",
    "astring": "^1.9.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^33.3.1",
    "jsdom": "^26.0.0",
    "object-inspect": "^1.13.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-simple-code-editor": "^0.14.1",
    "react-syntax-highlighter": "^15.6.1",
    "util": "^0.12.5",
    "wait-on": "^8.0.2"
