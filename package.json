{
  "name": "opents",
  "private": true,
  "version": "0.2.16",
  "description": "OpenTS",
  "author": {
    "name": "Ra1NuX",
    "email": "ra1nux.01@gmail.com"
  },
  "main": "./app/index.js",
  "scripts": {
    "dev": "vite",
    "build:electron": "bun ./esbuild.config.mjs",
    "build:renderer": "vite build",
    "build": "npm run build:renderer && npm run build:electron",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:electron": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && npm run build:electron\"",
    "start": "electronmon ./app",
    "dist": "npm run build && electron-builder --config electron-builder.config.js -w",
    "docker-build": "docker run --rm -ti --env-file <(env | grep -iE 'DEBUG|NODE_|ELECTRON_|YARN_|NPM_|CI|CIRCLE|TRAVIS_TAG|TRAVIS|TRAVIS_REPO_|TRAVIS_BUILD_|TRAVIS_BRANCH|TRAVIS_PULL_REQUEST_|APPVEYOR_|CSC_|GH_|GITHUB_|BT_|AWS_|STRIP|BUILD_') --env ELECTRON_CACHE='/root/.cache/electron' --env ELECTRON_BUILDER_CACHE='/root/.cache/electron-builder' -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.cache/electron:/root/.cache/electron "
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@monaco-editor/react": "^4.6.0",
    "@types/express": "^5.0.0",
    "@webcontainer/api": "^1.5.1",
    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "astring": "^1.9.0",
    "cross-env": "^7.0.3",
    "electron-log": "^5.3.0",
    "electron-serve": "^2.1.1",
    "electron-updater": "^6.3.9",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.2",
    "jsdom": "^26.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.0",
    "react-icons": "^5.4.0",
    "react-syntax-highlighter": "^15.6.1",
    "tailwindcss-aria-attributes": "^2.0.1",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/jsdom": "^21.1.7",
    "@types/object-inspect": "^1.13.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "wait-on": "^8.0.2",
    "concurrently": "^9.1.2",
    "vite": "^6.0.5",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8"
  }
}
